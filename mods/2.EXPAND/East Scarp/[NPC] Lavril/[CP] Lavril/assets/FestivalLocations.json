    //Festival Dialogue
    {
      "Action": "EditData",
      "Target": "data/festivals/spring13",
      "Entries": {
        "Lavril": "Em không biết thỏ có thể đẻ trứng...#$b#Liệu thỏ có nở ra từ trứng không?$7#$b#Như vậy có nghĩa là Void cũng có thể đẻ trứng phải không?$13",
        "Void": "Không, Lavril. Tôi không phải là thỏ thực sự. Do đó, tôi không thể đẻ trứng.#$e#Sigh."
      }
    },

    {
      "Action": "EditData",
      "Target": "data/festivals/spring24",
      "Entries": {
        "Lavril": "Vincent nói rằng cậu ấy muốn trở thành nữ hoàng hoa tiếp theo. Em hy vọng một ngày nào đó cậu ấy sẽ chiến thắng.$16",
        "Void": "Nếu tôi là nữ hoàng hoa, tôi sẽ khiến mọi người phải cúi đầu và tôn thờ tôi.#$b#Những thần dân trung thành của tôi sẽ quỳ lạy trước mặt tôi, ca ngợi vùng đất mà tôi đặt chân đến.#$e#'Xin chào Dunn Nemecia.'$u#$b#%Void bắt đầu cười khùng khục."
      }
    },

    {
      "Action": "EditData",
      "Target": "data/festivals/summer11",
      "Entries": {
        "Lavril": "Ừm... Chúng em đã hái một ít anh đào trước đó và cho vào nồi. Em hy vọng mọi người thích chúng...$3",
        "Void": "Tốt hơn hết là đừng thêm bất cứ thứ gì kỳ lạ vào nồi. Tôi đang theo dõi cậu đấy."
      }
    },

    {
      "Action": "EditData",
      "Target": "data/festivals/summer28",
      "Entries": {
        "Lavril": "Những con sứa ánh trăng phát sáng trong bóng tối, đúng không?$4?#$b#Nếu những ngôi sao lại lạc trên bờ biển một lần nữa, có lẽ sứa có thể giúp dẫn đường cho chúng trở lại bầu trời đêm.$4",
        "Void": "...Tôi đoán là có rất ít cảnh đẹp đáng tham quan trên thế giới này."
      }
    },

    {
      "Action": "EditData",
      "Target": "data/festivals/fall16",
      "Entries": {
        "Lavril": "Tôi vừa xem bói xong... *hic*$9",
        "Void": "Bà thầy bói đó... Tôi nhớ những gì họ đã nói với Lavril.#$b#'Bạn sẽ gặp vô số người sẽ yêu quý bạn. Chẳng bao lâu nữa, bạn sẽ gặp những người bạn mà bạn đã chờ đợi.'"
      }
    },

    {
      "Action": "EditData",
      "Target": "data/festivals/fall27",
      "Entries": {
        "Lavril": "Void.. Quay lại đây ngay lập tức!$5#$b#Tôi không muốn gặp rắc rối!$5",
        "Void": "Những bộ xương đáng sợ...! Khiến bạn rùng mình~!"
      }
    },

    {
      "Action": "EditData",
